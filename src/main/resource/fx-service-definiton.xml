<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<vad:Definition xmlns:vad="http://www.vaplication.com/definition" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.vaplication.com/definition vapplication-definitons.xsd ">
    <vad:beans>
        <vad:bean id="genericControllerBuilder" class="main.java.leiDina.tec.javafx.factory.builder.GenericControllerBuilder"/>

        <vad:bean id="controllerFactory" class="main.java.leiDina.tec.javafx.factory.ControllerFactoryImpl">
            <vad:property propertyName="buiders">
                <vad:mapProperty keyType="class">
                    <vad:entry key="java.lang.Object">
                        <vad:beanProperty ref="genericControllerBuilder"/>
                    </vad:entry>
                </vad:mapProperty>
            </vad:property>
        </vad:bean>

        <vad:bean id="textInputNodeAssociation" class="main.java.leiDina.tec.javafx.service.TextInputNodeAssociation"/>
        <vad:bean id="numberInputNodeAssociation" class="main.java.leiDina.tec.javafx.service.NumberInputNodeAssociation"/>

        <vad:bean id="modelSceneWire" class="main.java.leiDina.tec.javafx.service.ModelSceneWire">
            <vad:property propertyName="associationMap">
                <vad:mapProperty keyType="class">
                    <vad:entry key="main.java.leiDina.tec.javafx.annotation.TextInput">
                        <vad:beanProperty ref="textInputNodeAssociation"/>
                    </vad:entry>
                    <vad:entry key="main.java.leiDina.tec.javafx.annotation.NumberInput">
                        <vad:beanProperty ref="numberInputNodeAssociation"/>
                    </vad:entry>
                </vad:mapProperty>
            </vad:property>
        </vad:bean>
    </vad:beans>
</vad:Definition>